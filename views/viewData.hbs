


<!DOCTYPE html>
<html>
<head>
  <title>Sales Data</title>
  <style>
    .image-cell img {
      max-width: 100px;
      max-height: 100px;
    }
  </style>
</head>
<body>
  <h1>Sales Data</h1>
  <table>
    <thead>
      <tr>
        {{#each sales.[0]}}
          <th>{{@key}}</th>
        {{/each}}
      </tr>
    </thead>
    <tbody>
      {{#each sales}}
        <tr>
          {{#each this}}
            <td>{{{im this}}}</td>
          {{/each}}
        </tr>
      {{/each}}
    </tbody>
  </table>
</body>
</html>

